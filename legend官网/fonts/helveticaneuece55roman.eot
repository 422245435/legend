<script>

	var hash = window.location.hash;
	$(function(){
		setLocation(hash);
	});
	function setLocation(hash) {
		var tm = arguments[1] ? arguments[1] : 500;
		if (hash.substring(0, 1) == '#') hash = hash.substring(1);
		else {
			var arr = hash.split('#');
			hash = arr[1];
		}
		if ( hash != '' ) {
			var obj = $('a[name="'+ hash +'"]');
			if ( obj.length > 0 ) {
				$('html,body').stop(true,true).animate({ 'scrollTop': obj.offset().top }, tm, function(){
					$('html,body').stop(true,true).animate({ 'scrollTop': obj.offset().top - $('#nav-copy').outerHeight(false) }, tm / 2);
				});
			}
		}
	}
</script>
