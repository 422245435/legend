<script>
	var myOptions = {
		zoom : 18,
		mapTypeId : google.maps.MapTypeId.ROADMAP
	};
	var bigMap = null;
	var directionsDisplay;
	var directionsService = new google.maps.DirectionsService();

	function openMapWindow() {
		var layerObj = $.layer({
			type : 1,
			title : false,
			fix : false,
			offset:['50px' , ''],
			area : ['90%','auto'],
			page : {dom : '#layerBox'}
		});
		var markerLocation = null;
		//生成地图
		bigMap = new google.maps.Map(document.getElementById('mapContainer'), myOptions);
		var marker = null;

		markerLocation = new google.maps.LatLng(-37.9160978,145.14866889999996);
		marker = new google.maps.Marker({
			position: markerLocation,
			map: bigMap
		});
		bigMap.setCenter(markerLocation);
	}

	function searchDirection() {
		var _startObj = $('#layerBox input[name="start"]');
		var _finishObj = $('#layerBox input[name="finish"]');
		if ( _startObj.val() == '' || _finishObj.val() == '' ) {
			alert('Please enter start and finish address!');
			return false;
		}

		directionsDisplay = new google.maps.DirectionsRenderer();
		directionsDisplay.setMap(bigMap);
		var request = {
			origin: _startObj.val(),
			destination: _finishObj.val(),
			travelMode: google.maps.DirectionsTravelMode.DRIVING
		};
		directionsService.route(request, function(response, status){
			if (status == google.maps.DirectionsStatus.OK) {
				directionsDisplay.setDirections(response);
			}
		});
	}
</script>
<div id="layerBox" class="layerBox">
	<div class="wrapper clearfix">
		<div class="search">
			<span><label>Start</label><input type="text" name="start" /></span>
			<span><label>Finish</label><input type="text" name="finish" value="30/195 Wellington Rd Clayton VIC Melbourne 3168 Australia" /></span>
			<button type="button" onclick="searchDirection(); return false;">Search</button>
		</div>
		<div class="map-content">
			<div id="mapContainer"></div>
		</div>
	</div>
</div>

</body>
