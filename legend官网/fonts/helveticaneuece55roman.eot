	$(window).scroll(function(){
		var _this = $('.featured-works .bd .pic-list');
		if( $('.featured-works .bd').offset().top <= (parseInt($(window).scrollTop()) + parseInt(($(window).height() / 3))) && featScroll ){
			featScroll = false;
			var randArray = [];
			var isRepeat = false;
			var plays = function(){
				for(var i=0; i<=randArray.length-1; i++ ){
					if(_this.find('.item').eq(randArray[i]).find('video').length > 0){
						_this.find('.item').eq(randArray[i]).find('video').show()
						_this.find('.item').eq(randArray[i]).find('video').get(0).play();
					}
				}
			};
			var isRepeatFn = function(){
				if( randArray.length >= 4){
					plays();
					return false;
				}else if(randArray.length < 1){
					var randValue = fwrandom();
					randArray.push(randValue);
					isRepeatFn();
				}else{
					isRepeat = false;
					var randValue = fwrandom();
					for(var i=0; i<=randArray.length-1; i++ ){
						if( randValue == randArray[i] ){
							isRepeat = true;
						}
					}
					if (isRepeat == false){
						randArray.push(randValue);
					}
					isRepeatFn();
				}
			};
			isRepeatFn();
		}
	});
	
